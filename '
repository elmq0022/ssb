package utils_test

import (
	tu "ssb/internal/commands/testUtils"
	"ssb/internal/commands/utils"
	"testing"
)


func TestBuildEndpoint(t *testing.T){
	tests := []struct {
		name string
		want string
		parts []string
	}{
		{
			name: "empty",
			want: "base-url",
			parts: []string{},
		},
	}
	cfg := utils.CLIConfig{
		URL: "base-url",
		Username: "username",
	}
	tu.SetConfig(t, cfg)
	got := utils.BuildEndpoint()
	if "base-url" != got{
		t.Fatalf("want %s, got %s", "base-url", got)
	}
}
